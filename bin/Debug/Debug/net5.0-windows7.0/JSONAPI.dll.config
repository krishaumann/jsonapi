			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="CefSharp.Core.Runtime" processorArchitecture="amd64" publicKeyToken="40c4b6fc221f4138" culture="neutral"/>
				<codeBase version="97.1.60.0" href="x64/CefSharp.Core.Runtime.dll"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
